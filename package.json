{
  "name": "picgo-plugin-vscode-migrator",
  "version": "0.0.2",
  "description": "Import images from vs-picgo",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/upupming/picgo-plugin-vscode-migrator#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p .",
    "dev": "tsc -w -p .",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags",
    "fix": "standard --fix **/*.ts"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "keywords": [
    "picgo",
    "picgo-gui-plugin",
    "picgo-plugin"
  ],
  "author": "upupming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/upupming/picgo-plugin-vscode-migrator/issues"
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "electron": "^9.1.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "picgo": "^1.2.0",
    "standard": "^12.0.1",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^22.0.0"
  }
}
